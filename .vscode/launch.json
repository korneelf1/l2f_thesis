{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Example: Test",
            "type": "debugpy",
            "request": "launch",
            "program": "examples/test.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "OMP_NUM_THREADS": "16"
            }
        },
        {
            "name": "Example: UI",
            "type": "debugpy",
            "request": "launch",
            "program": "examples/ui.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "OMP_NUM_THREADS": "16"
            }
        },
        {
            "name": "SB3 Wrapper",
            "type": "debugpy",
            "request": "launch",
            "program": "l2f/wrappers/sb3.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "OMP_NUM_THREADS": "16"
            }
        },
        {
            "name": "UI Server",
            "type": "debugpy",
            "request": "launch",
            "program": "l2f/ui_server.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "OMP_NUM_THREADS": "16"
            }
        },
        {
            "name": "Example: SB3",
            "type": "debugpy",
            "request": "launch",
            "program": "examples/sb3.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "OMP_NUM_THREADS": "1",
                "CUDA_VISIBLE_DEVICES": ""
            }
        }
    ]
}